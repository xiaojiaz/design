<view class='container'>
  <image class='images' src="{{lab.imageUrl}}"></image>
    <view class='section'>
      <text>实验室编号:</text>
      <input disabled="true" type='number'  name="no" value="{{lab.lab_No}}"/>
    </view>
    <view class='section'>
      <text>位置:</text>
      <input disabled="true" name="pwd" value="{{lab.lab_location}}"/>
    </view>
    <view class='section'>
      <text>可容纳人数:</text>
      <input disabled="true" name="pwd" value="{{lab.lab_Size}}"/>
    </view>
    <view class='section'>
      <text>描述:</text>
      <input disabled="true" name="pwd" value="{{lab.describe}}"/>
    </view>
    <view class='section'>
      <text>开始时间:</text>
      <input disabled="true" name="pwd" value="{{s_time}}"/>
    </view>
    <view class='section'>
      <text>结束时间:</text>
      <input disabled="true" name="pwd" value="{{e_time}}"/>
    </view>
    <view class='section'>
      <text>备注:</text>
      <textarea disabled="true" value="{{lab_brief}}"></textarea>
    </view>
    <view class="shoucang" bindtap="shoucang">收藏</view>
    <view class='button'>
      <button bindtap="gotoindex" type='primary' form-type='submit'>预约</button>
    </view>
</view>